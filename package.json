    "./solid": {
      "types": "./dist/solid/Slot.d.ts",
      "default": "./dist/solid/Slot.jsx"
    }
  },
  "scripts": {
    "build": "tsc",
    "check:builds": "bash ./scripts/check-builds.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maybebot/named-slots"
  },
  "author": "Ian Bobinac",
  "license": "MIT",
  "description": "Named slots for preact, react and solid",
  "devDependencies": {
    "@types/node": "^22.13.10",
    "prettier": "3.6.2",
    "typescript": "^5.8.2"
  }
}
