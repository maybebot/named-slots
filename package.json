{
  "name": "named-slots",
  "version": "0.4.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "slots",
    "named",
    "preact",
    "react",
    "solid"
  ],
  "main": "./dist/Slot.jsxs",
  "module": "./dist/Slot.jsx",
  "exports": {
    ".": {
      "types": "./dist/Slot.d.ts",
      "default": "./dist/Slot.jsx"
